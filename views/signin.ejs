<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title></title>
</head>
<body>
    
    <section class="section-one">
        <!-- do not remove this. this is the background -->
    
    </section>
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
        <strong>*  Fields  Required</strong>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
      </div>
    <section class="col-sm-8 offset-sm-2 section-two pt-5 pb-5 animate__animated animate__jello">
        <h1 class="text-center font-weight-bold sign-up-logo pb-2">Sign In</h1>
        <div class="row align-items-center">
            <div class="col-sm-8 offset-sm-2">
                <form action="/createSession" method="POST">
                    <div class="form-group">
                        
                        <label for="InputEmail">Email</label><span>*</span>
                        <input type="email" class="form-control" id="InputEmail" aria-describedby="emailHelp"
                            placeholder="abc@xyz.com" name="email" required>
                    </div>
                    <div class="form-group">
                        <label for="InputPassword">Password</label><span>*</span>
                        <input type="password" class="form-control" id="InputPassword" required name="password">
                    </div>
                
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
              
      
           
          
      
            </div>
        </div>
    </section>
</body>
</html>